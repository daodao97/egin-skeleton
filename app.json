{
    "name": "egin",
    "address": "127.0.0.1:8080",
    "mode": "<GIN_MODE>",
    "custom": [
        1,
        2,
        3
    ],
    "logger": {
        "type": "stdout",
        "fileName": "tmp/app.log",
        "level": 5
    },
    "database": {
        "default": {
            "host": "localhost",
            "port": 3306,
            "user": "root",
            "passwd": "root",
            "database": "hyperf_admin",
            "pool": {
                "maxOpenConns": 100,
                "maxIdleConns": 5
            },
            "options": {}
        }
    },
    "redis": {
        "default": {
            "host": "localhost",
            "port": 6379
        }
    },
    "rabbitmq": {
        "default": {
            "host": "localhost",
            "port": 5672,
            "user": "guest",
            "passwd": "guest",
            "vhost": "egin"
        }
    },
    "mongo": {
        "default": {
            "url": "mongodb://localhost:27017"
        }
    },
    "kafka": {
        "default": "localhost::9092"
    },
    "auth": {
        "cors": {
            "enable": false
        },
        "ipAuth": {
            "enable": true,
            "allowedIpList": [
                "127.0.0.1"
            ]
        },
        "ipLimiter": {
            "enable": false,
            "ipLimit": {
                "127.0.0.1": 1
            }
        },
        "aksk": {
            "enable": false,
            "allowed": {
                "123": "abc"
            }
        }
    },
    "consul": "http://127.0.0.1:8500"
}
